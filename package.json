{
  "name": "imagemanager",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "./run-script.sh",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "rubegartor <rubegartor@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.5.1",
    "electron-rebuild": "^1.10.1",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "dir2Json": "https://github.com/rubegartor/dir-to-json.git",
    "directory-tree": "^2.2.4",
    "drivelist": "^8.0.10",
    "electron-tree-view": "^1.1.0",
    "exif": "^0.6.0",
    "jquery": "^3.5.0",
    "jstree": "^3.3.9",
    "node-sass": "^4.13.1",
    "usb-detection": "^4.9.0"
  },
  "build": {
    "appId": "imagemanager",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "app/assets/icons/icon.ico"
    }
  }
}
